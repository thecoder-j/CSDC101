#include <iostream>
#include <string>
using namespace std;

int main() {

  int matrix [2][2] = { {1,2},
                        {3,4}};
  
  cout << " matrix[0][0] = " << matrix[0][0] << endl;
  cout << " matrix[1][0] = " << matrix[0][1] << endl;
  cout << " matrix[0][1] = " << matrix[1][0] << endl;
  cout << " matrix[1][1] = " << matrix[1][1] << endl; 
  
return 0;
}
