#include <iostream>
using namespace std;

int main() {

    int number, choice;
    
    
    do {
      cout << "Enter a number: ";
      cin >> number;
    
      for (int i = 1; i <= 10; i++){
      cout << i << "x" << number << "=" << i*number << endl;
      }
    
      cout << "Do you want to try another number? (1) Yes (2) No: ";
      
      cin >> choice;
      cout << endl;
    }
    while (choice == 1);
    
    cout << "Thank you for using the program" << endl;
    
    return 0;
    }
