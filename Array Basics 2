#include <iostream>
#include <string>
using namespace std;

int main() {
  string clothes [ ] = {"Uniqlo" , "Penshoppe" , "RRJ" , "ForMe" , "ChromeHearts" };
  
  cout << "the first brand is " << clothes [1] << endl;
  clothes [0] = "Tactics";
  cout << "the first clothing brand is " << clothes [5];
  
return 0;
}
